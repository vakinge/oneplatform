<?xml version="1.0" encoding="UTF-8"?>
<configuration status="INFO" monitorInterval="1800" shutdownHook="disable">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT" follow="true">
			<PatternLayout
				pattern="%date{yyyy-MM-dd HH:mm:ss.SSS} %level [%thread][%file:%line] - %msg%n" />
		</Console>
	</appenders>

	<loggers>
		<logger name="com.zy" level="debug"></logger>
		<logger name="com.zyframework" level="debug"></logger>
		<logger name="com.jeesuite" level="debug"></logger>
		<logger name="org.mybatis.generator.internal.db.DatabaseIntrospector" level="DEBUG"></logger>
		<root level="info">
			<appender-ref ref="Console" />
		</root>
	</loggers>

</configuration>